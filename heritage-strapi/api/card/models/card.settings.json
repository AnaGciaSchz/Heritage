{
  "kind": "collectionType",
  "collectionName": "cards",
  "info": {
    "name": "Card"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "IdCard": {
      "type": "uid"
    },
    "NombreCompletoCard": {
      "type": "string",
      "required": true,
      "minLength": 5
    },
    "ImagenDePerfilCard": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {}
    },
    "FechaAltaCard": {
      "type": "date",
      "required": true
    },
    "Red1Card": {
      "type": "string",
      "required": false,
      "minLength": 5
    },
    "Red2Card": {
      "type": "string",
      "minLength": 5
    },
    "Red3Card": {
      "type": "string",
      "minLength": 5
    }
  }
}
